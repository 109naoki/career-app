generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum Role {
  USER
  ADMIN
}

model User {
  id            String    @id @default(cuid())
  age           Int?
  name          String
  email         String    @unique
  emailVerified DateTime?
  password      String
  role          Role      @default(USER)
}
